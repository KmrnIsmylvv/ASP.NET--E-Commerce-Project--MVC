@model HomeVm
@{
    ViewData["Title"] = "Index";
    int count = 1;
    int productcount = 1;
}

<!--====== Slider Part Start ======-->

<section class="slider-area slider-active">
    <div class="single-slider">
        <div class="slider-image">
            <img src="assets/images/slider-1.jpg" alt="slider">
        </div>
        <div class="slider-content-wrapper">
            <div class="container-fluid custom-container">
                <div class="slider-content">
                    <h4 data-animation="fadeInDown" data-delay="0.5s" class="sub-title">Save $120 when you buy</h4>
                    <h1 data-animation="fadeInLeft" data-delay="0.5s" class="main-title "><span>2020 Virtual Reality </span> Fulldive VR.</h1>
                    <p data-animation="fadeInRightBig" data-delay="0.5s">Explore and immerse in exciting 360 content with Fulldive’s all-in-one virtual reality platform</p>
                    <a data-animation="zoomIn" data-delay="0.5s" class="main-btn " href="single-product.html">Shop Now</a>
                </div>
            </div> <!-- container -->
        </div>
    </div> <!-- single slider -->
    <div class="single-slider">
        <div class="slider-image">
            <img src="assets/images/slider-2.jpg" alt="slider">
        </div>
        <div class="slider-content-wrapper">
            <div class="container-fluid custom-container">
                <div class="slider-content">
                    <h4 data-animation="fadeInDown" data-delay="0.5s" class="sub-title">Save $120 when you buy</h4>
                    <h1 data-animation="fadeInLeft" data-delay="0.5s" class="main-title "><span>4K HDR Smart TV 43 </span> Sony Bravia.</h1>
                    <p data-animation="fadeInRightBig" data-delay="0.5s">Explore and immerse in exciting 360 content with Fulldive’s all-in-one virtual reality platform</p>
                    <a data-animation="zoomIn" data-delay="0.5s" class="main-btn " href="single-product.html">Shop Now</a>
                </div>
            </div> <!-- container -->
        </div>
    </div> <!-- single slider -->
</section>

<!--====== Slider Part Ends ======-->
<!--====== Banner Part Start ======-->

<div class="banner-area banner-bg">
    <div class="container-fluid custom-container">
        <div class="row">
            <div class="col-md-6">
                <div class="banner-image">
                    <a href="shop-4-column.html"><img src="assets/images/banner-1.png" alt="banner"></a>
                </div> <!-- banner image -->
            </div>
            <div class="col-md-6">
                <div class="banner-image">
                    <a href="shop-4-column.html"><img src="assets/images/banner-2.png" alt="banner"></a>
                </div> <!-- banner image -->
            </div>
        </div> <!-- row -->
    </div> <!-- container -->
</div>

<!--====== Banner Part Ends ======-->
<!--====== Category Part Start ======-->

<section class="product-area pt-100">
    <div class="container-fluid custom-container">
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="section-title text-center pb-30">
                    <h2 class="title">Featured Categories</h2>
                </div>
            </div>
        </div> <!-- row -->

        <div class="row-wrapper category-active">
            <div class="custom-col">
                @foreach (var category in Model.categories)
                {
                    <div class="single-category text-center">
                        <a target="_blank" href="shop-left-sidebar.html">
                            <img src="~/assets/images/@category.ImageUrl" style="width:133px;height:133px" alt="category">
                            <p>@category.Name</p>
                        </a>
                    </div>
                    if (count % 2 == 0 && count != Model.categories.Count)
                    {
                        @Html.Raw("</div>");
                        @Html.Raw("<div class='custom-col'>");
                    }
                    count++;
                }
            </div>
        </div>
    </div><!-- container -->
</section>

<!--====== Category Part Ends ======-->
<!--====== Product Part Start ======-->

<section class="product-area pt-100 pb-100">
    <div class="container-fluid custom-container">
        <div class="row">
            <div class="col-lg-12">
                <div class="product-menu pb-30">
                    <ul class="nav justify-content-center" id="myTab" role="tablist">
                        <li class="nav-item" role="presentation">
                            <a class="active" id="new-tab" data-toggle="tab" href="#new" role="tab" aria-controls="new" aria-selected="true">New Arrival</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a id="bestseller-tab" data-toggle="tab" href="#bestseller" role="tab" aria-controls="bestseller" aria-selected="false">Bestseller</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a id="featured-tab" data-toggle="tab" href="#featured" role="tab" aria-controls="featured" aria-selected="false">Featured</a>
                        </li>
                    </ul>
                </div> <!-- product menu -->
            </div>
        </div> <!-- row -->
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="new" role="tabpanel" aria-labelledby="new-tab">
                <div class="product-items">
                    <div class="row product-active">
                        <div class="col-lg-2">
                            @foreach (var item in ViewBag.newarrive)
                            {
                                <div class="single-product mt-30">
                                    <div class="product-image">
                                        <a href="single-product.html">
                                            <img class="first-image" src="~/assets/images/product/@item.Images[0].ImageUrl" alt="product">
                                            @if (item.Images.Count > 1)
                                            {
                                                <img class="second-image" src="~/assets/images/product/@item.Images[1].ImageUrl" alt="product">
                                            }
                                        </a>
                                        <ul class="product-meta">
                                            <li><a asp-controller="Basket" asp-action="AddBasket" asp-route-id="@item.Id"><i class="icon ion-bag"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-favorite-outline"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-options"></i></a></li>
                                            <li><a data-toggle="modal" data-target="#productQuickModal" href="#"><i class="icon ion-android-open"></i></a></li>
                                        </ul>
                                        @if (item.Campaign.Discount != 0)
                                        {
                                            <span class="discount-product">-@item.Campaign.Discount%</span>
                                        }
                                    </div>
                                    <div class="product-content">
                                        <ul class="product-rating">
                                            <li class="rating-on"><i class="fas fa-star"></i></li>
                                            <li class="rating-on"><i class="fas fa-star"></i></li>
                                            <li class="rating-on"><i class="fas fa-star"></i></li>
                                            <li class="rating-on"><i class="fas fa-star"></i></li>
                                            <li class="rating-on"><i class="fas fa-star"></i></li>
                                        </ul>
                                        @{
                                            string ShortDesc = item.Description;
                                            if (ShortDesc.Length > 54)
                                            {
                                                ShortDesc = ShortDesc.Substring(0, 54) + "...";
                                            }
                                        }
                                        @{
                                            double salePrice = 0;
                                            @if (item.Campaign.Discount != 0)
                                            {
                                                salePrice = Convert.ToDouble(item.Price - ((item.Price) * (item.Campaign.Discount) / 100));
                                            }

                                        }
                                        <h4 class="product-title"><a href="single-product.html">@Html.Raw(ShortDesc)</a></h4>
                                        <div class="product-price">
                                            @if (item.Campaign.Discount == 0)
                                            {
                                                <span class="price-sale">€@item.Price</span>
                                            }
                                            @if (item.Campaign.Discount != 0)
                                            {
                                                <span class="regular-price ">€@item.Price</span>
                                                <span class="price-sale">€@salePrice</span>
                                            }

                                        </div>
                                    </div>
                                </div> <!-- single product -->
                                @if (productcount % 2 == 0 && productcount != ViewBag.newarrive.Count)
                                {
                                    @Html.Raw("</div>");
                                    @Html.Raw("<div class='col-lg-2'>");
                                }
                                productcount++;
                            }
                        </div>
                    </div> <!-- row -->
                </div> <!-- product items -->
