<section class="blog-area blog-bg pt-100 pb-100">
    <div class="container">
        <div class="row blog-active">

            @foreach (var item in Model.Blogs)
            {
                <div class="col-lg-4">
                    <div class="single-blog">
                        <div class="blag-image">

                            @foreach (var element in ViewBag.photos)
                            {
                                if (element.BlogId == item.Id && element.PhotoUrl != null)
                                {
                                    <div class="gallery-item">
                                        <a asp-controller="Blog" asp-action="Details" asp-route-id="@item.Id"><img src="~/assets/images/@element.PhotoUrl" style="width:350px" height="258" alt="blog"></a>
                                    </div>
                                }

                                if (element.BlogId == item.Id && element.VideoUrl != null)
                                {
                                    <div class="blog-post-video">
                                        <iframe src="@element.VideoUrl" allow="autoplay; encrypted-media" allowfullscreen="" width="560"></iframe>
                                    </div>
                                }
                            }
                            <p class="meta-date">
                                <span class="day">@item.Date</span>
                            </p>
                        </div>
                        <div class="blag-content">
                            <h2 class="blog-title"><a href="blog-single-left-sidebar.html">@item.Title</a></h2>
                            <p>
                                @{
                                    string ShortDesc = item.Description;
                                    if (ShortDesc.Length > 218)
                                    {
                                        ShortDesc = ShortDesc.Substring(0, 218) + "...";
                                    }
                                }
                                @Html.Raw(ShortDesc)
                            </p>
                        </div>
                    </div> <!-- single blog -->
                </div>
            }
        </div>
    </div> <!-- row -->
    <!-- container -->
</section>

<!--====== Blog Part Ends ======-->
